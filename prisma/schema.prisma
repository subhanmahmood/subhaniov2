datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
 
generator client {
  provider = "prisma-client-js"
}

model Link {
    id       Int      @id @default(autoincrement())
    name     String
    url      String
    category String

    clicks   LinkClick[]

    @@index([name])
}

model LinkClick {
    id       Int      @id @default(autoincrement())
    linkId   Int
    datetime DateTime @default(now())

    link     Link     @relation(fields: [linkId], references: [id])

    @@index([linkId])
}